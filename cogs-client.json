{
  "processors": {
    "vert": "txt",
    "frag": "txt"
  },
  "options": {
    "concatAmd": true,
    "names": {
      "box2d": "bower_components/box2d/box2d.umd.js",
      "cursors": "bower_components/cursors/cursors.js",
      "fastclick": "bower_components/fastclick/lib/fastclick.js",
      "jquery": "client/jquery.es6",
      "react": "bower_components/react/react-with-addons.min.js",
      "react-router": "bower_components/react-router/dist/react-router.min.js",
      "socket.io": "bower_components/socket.io-client/socket.io.js",
      "superagent": "bower_components/superagent/superagent.js",
      "three": "bower_components/threejs/build/three.min.js",
      "underscore": "bower_components/underscore/underscore-min.js"
    },
    "shims": {
      "three": {
        "global": "THREE"
      }
    },
    "processors.es6.options.compatFix": true,
    "compressors.js.ignore": [
      "box2d",
      "react",
      "react-router",
      "three",
      "underscore"
    ]
  },
  "targets": {
    "client/index.es6.jsx": "public",
    "styles/index.scss": "public"
  }
}
